  "require-dev": {
    "behat/behat": "^3.5",
    "behat/mink": "~1.7@dev",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-extension": "^2.3",
    "behat/symfony2-extension": "^2.1",
    "behatch/contexts": "^3.2",
    "bex/behat-skip-tests": "^1.2",
    "dg/bypass-finals": "^1.1",
    "friendsofphp/php-cs-fixer": "^2.15",
    "hautelook/alice-bundle": "^2.5",
    "justinrainbow/json-schema": "^5.2",
    "leanphp/behat-code-coverage": "^3.4",
    "phpmd/phpmd": "^2.7",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpstan/phpstan-symfony": "^1.2",
    "phpstan/phpstan-webmozart-assert": "^1.2",
    "phpunit/php-code-coverage": "^6.1",
    "phpunit/phpcov": "^5.0",
    "phpunit/phpunit": "^7.5",
    "rector/rector": "^0.14.0",
    "slevomat/coding-standard": "^8.0",
    "squizlabs/php_codesniffer": "^3.7",
    "symfony/browser-kit": "^4.4",
    "symfony/css-selector": "^4.4",
    "symfony/maker-bundle": "^1.13",
    "symfony/phpunit-bridge": "^5.3",
    "symfony/stopwatch": "^4.4",
    "symfony/web-profiler-bundle": "^4.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:brouckerw1/doctrine2-spatial.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:sweetspotio/FOSElasticaBundle.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/theofidry/AliceBundle"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "php-http/discovery": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "refresh-db": [
      "php bin/console doctrine:database:drop --force",
      "php bin/console doctrine:database:create",
      "php bin/console doctrine:database:import ./dump/default_localhost-2023_04_15_02_26_00-dump.sql",
      "php bin/console doctrine:migrations:migrate -n",
      "php bin/console hautelook:fixtures:load -n --no-debug",
      "php bin/console fos:elastica:populate --no-debug --max-per-page=20"
    ],
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true",
      "require": "4.4.*",
      "docker": true
    }
  }
}
